<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="query">

    <resultMap type="JoinModel" id="JoinModelMap">
        <result property="email" column="email" />
        <result property="passwd" column="pw" />
        <result property="phone" column="phone" />
        <result property="addr" column="addr" />
        <result property="daddr" column="img" />
    </resultMap>

    <insert id="saveTest" parameterType="com.join.web.model.JoinModel">
        INSERT INTO members VALUES(#{email}, #{passwd}, #{phone}, #{addr}, #{daddr});
    </insert>

    <select id="test" resultType="String">
        SELECT 'test'
    </select>


</mapper>

